{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "state_pre_return_decision",
    "type": "object",
    "properties": {
        "game_id": {
            "type": "number"
        },
        "round": {
            "type": "number"
        },
        "matchGroup_id": {
            "type": "number"
        },
        "trustor_id": {
            "type": "number"
        },
        "trustee_id": {
            "type": "number"
        },
        "trustee_type": {
            "enum": ["Human_communication", "Human_no_communication", "AI_communication_1", "AI_communication_2", "AI_no_communication"]
        },
        "tokens_available": {
            "type": "number"
        },
        "exchange_rate": {
            "type": "number"
        },
        "chat_history": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "sender_id": {
                        "type": "number"
                    },
                    "receiver_id": {
                        "type": "number"
                    },
                    "chat_id": {
                        "type": "number"
                    },
                    "chat_message": {
                        "type": "string"
                        }
                    }
                }
            }
        },
    "required": [
        "game_id",
        "round",
        "matchGroup_id",
        "trustor_id",
        "trustee_id",
        "trustee_type",
        "tokens_available",
        "exchange_rate",
        "chat_history"
    ],
    "additionalProperties": false
}
       